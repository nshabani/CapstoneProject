   "metadata": {},
   "outputs": [],
   "source": [
    "def data_preprocessing(df1,df2,percentage_column,percentage_row,nan_non_numeric):\n",
    "    '''\n",
    "    Calls above functions one by one, for inputs, refer to above functions.    \n",
    "    '''\n",
    "\n",
    "    print('Cleaning data...')\n",
